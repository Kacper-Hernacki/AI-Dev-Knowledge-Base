{
  "name": "langchain-lesson",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx --watch --env-file=.env src/index.ts",
    "start": "tsx --env-file=.env src/index.ts",
    "typecheck": "tsc --noEmit",
    "agents:demo": "bun --env-file=.env lessons/langchain/core-concepts/agents/demo.ts",
    "agents:test": "bun test lessons/langchain/core-concepts/agents/tests/",
    "agents:test:watch": "bun test --watch lessons/langchain/core-concepts/agents/tests/",
    "agents:examples": "bun --env-file=.env lessons/langchain/core-concepts/agents/examples/index.ts",
    "models:demo": "bun --env-file=.env lessons/langchain/core-concepts/models/demo.ts",
    "models:test": "bun test lessons/langchain/core-concepts/models/tests/",
    "models:test:watch": "bun test --watch lessons/langchain/core-concepts/models/tests/",
    "models:examples": "bun --env-file=.env lessons/langchain/core-concepts/models/examples/index.ts",
    "messages:demo": "bun --env-file=.env lessons/langchain/core-concepts/messages/demo.ts",
    "messages:test": "bun test lessons/langchain/core-concepts/messages/tests/",
    "messages:test:watch": "bun test --watch lessons/langchain/core-concepts/messages/tests/",
    "messages:examples": "bun --env-file=.env lessons/langchain/core-concepts/messages/examples/index.ts",
    "tools:demo": "bun --env-file=.env lessons/langchain/core-concepts/tools/demo.ts",
    "tools:test": "bun test lessons/langchain/core-concepts/tools/tests/",
    "tools:test:watch": "bun test --watch lessons/langchain/core-concepts/tools/tests/",
    "tools:examples": "bun --env-file=.env lessons/langchain/core-concepts/tools/examples/index.ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:all": "bun test lessons/",
    "lessons:run": "echo 'Usage: bun run <lesson>:demo | <lesson>:test | <lesson>:examples'",
    "lessons:list": "ls -la lessons/"
  },
  "devDependencies": {
    "@types/bun": "^1.3.1",
    "@types/node": "^22.8.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@langchain/core": "^1.0.3",
    "@langchain/langgraph": "^1.0.1",
    "@langchain/openai": "^1.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "langchain": "^1.0.2",
    "tsx": "^4.20.6",
    "zod": "^4.1.12"
  }
}
